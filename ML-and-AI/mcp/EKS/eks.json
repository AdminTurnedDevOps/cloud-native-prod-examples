{
	"mcpServers": {
		"awslabs.eks-mcp-server": {
			"command": "uvx",
			"args": [
                "awslabs.eks-mcp-server",
                "--allow-write"
            ],
			"env": {
				"AWS_PROFILE": "default",
                "AWS_REGION": "us-west-2",
                "FASTMCP_LOG_LEVEL": "INFO"
			}
		}
	}
}